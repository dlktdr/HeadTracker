{
  "build": {
    "arduino": {
      "ldscript": "linker_script.ld"
    },
    "core": "nRF5",
    "cpu": "cortex-m4",
    "extra_flags": "-DARDUINO_ARDUINO_NANO33BLE_SENSE_R2 -DARDUINO_ARCH_NRF52840 -DNRF52840_XXAA",
    "f_cpu": "64000000L",
    "hwids": [
      [
        "0x2341",
        "0x005a"
      ],
      [
        "0x2341",
        "0x805a"
      ]
    ],
    "mcu": "nrf52840",
    "variant": "ARDUINO_NANO33BLE_SENSE_R2"
  },
  "connectivity": [
    "bluetooth"
  ],
  "debug": {
    "jlink_device": "nRF52840_xxAA",
    "openocd_target": "nrf52.cfg",
    "svd_path": "nrf52840.svd"
  },
  "frameworks": [
    "zephyr"
  ],
  "name": "Arduino Nano 33 BLE Sense R2",
  "upload": {
    "maximum_ram_size": 262144,
    "maximum_size": 983040,
    "protocol": "sam-ba",
    "protocols": [
      "jlink",
      "nrfjprog",
      "nrfutil",
      "cmsis-dap",
      "sam-ba",
      "blackmagic"
    ],
    "use_1200bps_touch": true,
    "require_upload_port": true,
    "wait_for_upload_port": true
  },
  "url": "https://docs.arduino.cc/hardware/nano-33-ble-sense-rev2",
  "vendor": "Arduino"
}